{
  "name": "@stream/server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "prisma generate && tsc",
    "start": "prisma db push --accept-data-loss --skip-generate && node dist/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:push:deploy": "prisma db push --accept-data-loss",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:clean": "node -e \"try{require('fs').rmSync('node_modules/.prisma',{recursive:true,force:true});}catch(e){console.error('Close dev server and Node processes first');process.exit(1);}\"",
    "db:generate:clean": "npm run db:clean && prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@stream/shared": "*",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.14.10",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "openai": "^4.104.0",
    "tesseract.js": "^7.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "prisma": "^5.22.0",
    "tsx": "^4.19.1",
    "typescript": "^5.3.3"
  }
}
